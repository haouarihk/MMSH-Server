{
    "name": "compiler Handler",
    "requirer": "converter-toless-plugin",
    "routerConfig": {
        "pages": {
            "main": {
                "name": "main",
                "dir": "main"
            },
            "message": {
                "name": "message",
                "dir": "message"
            }
        },
        "paths": {
            "main": "",
            "inputdir": "./input",
            "outputdir": "./output"
        }
    },
    "config": {
        "debug": false,
        "logInFile": true,
        "timetoGarbageCleaner": 1,
        "deleteallTempFilesOnStart": true,
        "filesizeLimitsMB": 100,
        "randomStringSize": 8,
        "compilers": [
            {
                "name": "Docx to Latex",
                "commander": "",
                "CompilerPath": "./compilers/doc2tex/d2t",
                "command": "-o \"#{oPath}\" \"#{iPath.type}\"",
                "whitelistInputs": [
                    "Docx"
                ]
            }
        ]
    }
}