{
    "name": "compiler Handler",
    "requirer": "converter-toless-plugin",
    "routerConfig": {
        "pages": {
            "main": {
                "name": "main",
                "dir": "main"
            },
            "message": {
                "name": "message",
                "dir": "message"
            }
        },
        "paths": {
            "main": "",
            "inputdir": "./input",
            "outputdir": "./output"
        }
    },
    "config": {
        "compilers": [
            {
                "name": "Word to Latex",
                "commander": "",
                "CompilerPath": "./compilers/doc2latex/d2t",
                "command": "-o \"#{oPath}\" \"#{iPath.type}\"",
                "whitelistInputs": [
                    "Doc",
                    "Docx"
                ]
            },
            {
                "name": "Doc to PDF",
                "commander": "python",
                "CompilerPath": "./compilers/word-to-pdf/main.py",
                "command": "-o \"#{CompeleteOutputFilePath}\" -i \"#{CompeleteInputFilePath}\"",
                "whitelistInputs": [
                    "Doc",
                    "Docx"
                ]
            }
        ]
    }
}